{
    "version": "4.0-beta1",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface (BETA release).",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta1/Godot_v4.0-beta1_win64.exe.zip",
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta1/Godot_v4.0-beta1_win32.exe.zip",
        }
    },
    "pre_install": "Get-ChildItem \"$dir\\Godot_*.exe\" | Rename-Item -NewName \"$dir\\godot.exe\"",
    "bin": "godot.exe",
    "shortcuts": [
        [
            "godot.exe",
            "Godot Engine"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/news",
        "regex": "Dev snapshot: Godot (?<ver>[\\d.]+) beta (?<alpha>[\\d.]+)",
        "replace": "${1}-beta${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/beta$matchAlpha/Godot_v$version_win64.exe.zip"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/beta$matchAlpha/Godot_v$version_win32.exe.zip"
            }
        }
    }
}
