{
    "version": "4.0-beta0",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface (BETA release).",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta0/Godot_v4.0-beta0_win64.exe.zip",
            "hash": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta0/Godot_v4.0-beta0_win32.exe.zip",
            "hash": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
        }
    },
    "pre_install": "Get-ChildItem \"$dir\\Godot_*.exe\" | Rename-Item -NewName \"$dir\\godot.exe\"",
    "bin": "godot.exe",
    "shortcuts": [
        [
            "godot.exe",
            "Godot Engine"
        ]
    ],
    "checkver": {
        "url": "https://downloads.tuxfamily.org/godotengine/4.0/",
        "regex": "beta(?<beta>[\\d.]+)",
        "replace": "4.0-beta${1}",
        "reverse": true
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta$matchBeta/Godot_v$version_win64.exe.zip"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/4.0/beta$matchBeta/Godot_v$version_win32.exe.zip"
            }
        }
    }
}
